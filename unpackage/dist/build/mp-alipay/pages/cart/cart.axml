<block a:if="{{cart.length>0}}"><view class="container"><view class="top"><scroll-view class="scroll-view_H" scroll-x="true" scroll-left="{{scrollLeft}}" a:for="{{cart}}" a:for-item="item" a:for-index="index" a:key="index"><view class="cartInfo" id="demo1"><view class="cartInfochild"><view class="cartInfo_switch common"><switch type="checkbox" checked="{{item.checked}}" data-event-opts="{{[['change',[['_check',['$event',index]]]]]}}" onChange="__e"></switch></view><view class="cartInfo_image common"><image src="{{item.img}}" mode="widthFix"></image></view><view class="cartInfo_info"><label style="font-size:26rpx;">{{item.goodsname}}</label><label style="font-size:24rpx;color:#ccc;">规格：不错的商品</label><label style="color:red;">{{"￥"+item.price}}</label></view><view class="cartInfo_num"><view class="cartInfo_num_child"><label data-event-opts="{{[['tap',[['_editCart',[index,1]]]]]}}" onTap="__e">-</label><label>{{item.num}}</label><label data-event-opts="{{[['tap',[['_editCart',[index,2]]]]]}}" onTap="__e">+</label></view></view></view></view><view class="cartDel" id="demo2" data-event-opts="{{[['tap',[['_delCart',[index]]]]]}}" onTap="__e"><label for>删除</label></view></scroll-view></view><view class="foot"><view class="footd1"><switch type="checkbox" data-event-opts="{{[['change',[['_checkAll',['$event']]]]]}}" onChange="__e"></switch><label>全选</label></view><view class="footd2"><view class="footd2sp1">总计：<label style="color:red;">{{sumPrice}}</label></view><label class="footd2sp2">不含运费，已优惠￥0.00</label></view><view data-event-opts="{{[['tap',[['_goConfirm',['$event']]]]]}}" class="footd3" onTap="__e"><label>{{"去结算("+sumNum+"件)"}}</label></view></view></view></block><block a:else><view><image src="/static/kk.jpg" mode="widthFix"></image></view></block>