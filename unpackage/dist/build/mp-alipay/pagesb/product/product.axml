<view><view class="search"><icon type="search" color="white" size="10"></icon><input type="text" value="" placeholder="搜索商品" placeholder-class="placeholder" data-event-opts="{{[['tap',[['_goSearch']]]]}}" onTap="__e"/></view><view class="list"><view data-event-opts="{{[['tap',[['_goDetail',['$0'],[[['goods','',index,'id']]]]]]]}}" class="row" onTap="__e" a:for="{{goods}}" a:for-item="item" a:for-index="index" a:key="index"><image src="{{item.img}}" mode="widthFix"></image><view class="info"><text>{{item.goodsname}}</text><text>{{"￥"+item.price}}</text><text>{{item.market_price}}</text></view></view></view></view>